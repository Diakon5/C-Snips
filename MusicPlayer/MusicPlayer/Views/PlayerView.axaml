<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="400
			 "
             x:Class="MusicPlayer.Views.PlayerView">
	<Grid ShowGridLines="True" ColumnDefinitions="*,*,*,*,*" VerticalAlignment="Center" RowDefinitions="Auto,*" Margin="50 0 50 0">
		<TextBlock Grid.Column="0" Grid.Row="0" TextAlignment="End" VerticalAlignment="Center" >TimeCurrent</TextBlock>
		<Slider Grid.Column="1" Grid.ColumnSpan="3" VerticalAlignment="Center" Grid.Row="0"></Slider>
		<TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center">TimeMax</TextBlock>
		<Button Grid.Column="0" Grid.Row="1" Margin="30">
			<Image Source="/Assets/avalonia-logo.ico" />
		</Button>
		<Button Grid.Column="1" Grid.Row="1" Margin="30">
			<Image Source="/Assets/avalonia-logo.ico" />
		</Button>
		<Button Grid.Column="2" Grid.Row="1" Margin="30">
			<Image Source="/Assets/avalonia-logo.ico" />
		</Button>
		<Button Grid.Column="3" Grid.Row="1" Margin="30">
			<Image Source="/Assets/avalonia-logo.ico" />
		</Button>
		<Button Grid.Column="4" Grid.Row="1" Margin="30">
			<Image Source="/Assets/avalonia-logo.ico" />
		</Button>
	</Grid>
</UserControl>
